{
  "hash": true,
  "publicPath": "./",
  "plugins": [
    "build-plugin-fast-refresh",
    [
      "build-plugin-moment-locales",
      {
        "locales": [
          "zh-cn"
        ]
      }
    ],
    [
      "build-plugin-css-assets-local",
      {
        "outputPath": "assets",
        "relativeCssPath": "../"
      }
    ],
    [
      "build-plugin-antd",
      {
        "themeConfig": {
          "primary-color": "#026d9b",
          "primary-1": "#026d9b",
          "btn-text-hover-bg": "rgba(0, 0, 0, 0.035)"
        }
      }
    ]
  ]
}
